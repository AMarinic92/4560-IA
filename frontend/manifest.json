{
    "manifest_version": 3,
    "name": "Accessibility checker",
    "version": "1.0.0",
   
    "side_panel": {
      "default_path": "index.html"
    },
    "permissions":[
      "sidePanel",
      "background.js"
      
    ],
    "host_permissions":[
      "http://*/*",
      "https://*/*"
    ]
  }